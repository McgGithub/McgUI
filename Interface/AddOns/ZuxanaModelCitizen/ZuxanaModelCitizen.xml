	<Frame name="ModelCitizen_BoundingBoxTemplate" frameStrata="LOW" virtual="true" hidden="true">
		<Size>
			<AbsDimension x="200" y="200"/>
		</Size>
		<Backdrop bgFile="Interface\Tooltips\UI-Tooltip-Background"/>
		<Frames>
			<PlayerModel name="$parent_Avatar" setAllPoints="true">
				<Size>
					<AbsDimension x="200" y="200"/>
				</Size>
				<Scripts>
					<OnLoad>
						ModelCitizen_Avatar_OnLoad();
					</OnLoad>
					<OnUpdate>
						ModelCitizen_Avatar_OnUpdate();
					</OnUpdate>
					<OnEvent>
						ModelCitizen_Avatar_OnEvent();
					</OnEvent>
				</Scripts>
			</PlayerModel>
		</Frames>
		<Scripts>
			<OnLoad>
				ModelCitizen_BoundingBox_OnLoad();
			</OnLoad>
			<OnEvent>
				ModelCitizen_BoundingBox_OnEvent();
			</OnEvent>
			<OnMouseDown>
				ModelCitizen_BoundingBox_OnMouseDown();
			</OnMouseDown>
			<OnUpdate>
				ModelCitizen_BoundingBox_OnUpdate();
			</OnUpdate>
			<OnReceiveDrag>
				ModelCitizen_BoundingBox_OnReceiveDrag();
			</OnReceiveDrag>
			<OnMouseUp>
				ModelCitizen_BoundingBox_OnMouseUp();
			</OnMouseUp>
			<OnShow>
				ModelCitizen_BoundingBox_OnShow();
			</OnShow>
			<OnHide>
				ModelCitizen_BoundingBox_OnHide();
			</OnHide>
		</Scripts>
	</Frame>
	
